{
  "name": "@tamagui-extras/ui",
  "license": "MIT",
  "author": "Dominic Garms",
  "version": "0.0.8",
  "repository": "https://github.com/dohomi/tamagui-kitchen-sink",
  "homepage": "https://tamagui-extras.vercel.app",
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "react",
    "react-native",
    "@tamagui/core",
    "tamagui",
    "react-hook-form"
  ],
  "types": "./types/index",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist types && tamagui-build src/index.tsx",
    "watch": "tamagui-build src/index.tsx --watch",
    "check-types": "tsc --noEmit",
    "release-minor": "yarn build && npx release-it minor",
    "release-patch": "yarn build && npx release-it patch",
    "release-major": "yarn build && npx release-it major"
  },
  "dependencies": {
    "@datepicker-react/hooks": "^2.8.4",
    "@mdi/js": "^7.0.96",
    "@my/config": "*",
    "react-hook-form": "^7.34.2",
    "react-native-svg": "^12.4.4",
    "rooks": "^7.4.0",
    "solito": "^1.0.12",
    "tamagui": "1.0.1-beta.181"
  },
  "devDependencies": {
    "@storybook/react": "^6.5.12",
    "@tamagui/build": "1.0.1-beta.181"
  },
  "publishConfig": {
    "access": "public"
  }
}
